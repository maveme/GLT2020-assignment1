<?xml version="1.0" encoding="UTF-8"?>
<ccl:CloudResources
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ccl="http://glt.tue.nl/ccl"
    xsi:schemaLocation="http://glt.tue.nl/ccl ../model/ccl.ecore">
  <resources
      label="Resource 1">
    <machineImages
        xsi:type="ccl:ComputingImage"
        label="Computing machine image 1.1 (California)"
        vcpu="23"
        memory="21"
        storageSize="22"/>
    <machineImages
        xsi:type="ccl:ComputingImage"
        label="Computing machine image 1.2 (California)"
        vcpu="33"
        memory="31"
        storageSize="32"
        os="WINDOWS"/>
    <machineImages
        xsi:type="ccl:StorageImage"
        label="Storage machine image 1.1 (California)"
        vcpu="12"
        memory="10"
        storageSize="11"
        storageType="SSD"
        dbEngine="POSTGRESQL"/>
  </resources>
  <resources
      label="Resource 2">
    <machineImages
        xsi:type="ccl:StorageImage"
        region="FRANKFURT"
        label="Storage machine image 2.1 (Frankfurt)"
        vcpu="6"
        memory="4"
        storageSize="5"
        dbEngine="ORACLE"/>
    <machineImages
        xsi:type="ccl:ComputingImage"
        region="FRANKFURT"
        label="Computing machine image 2.1 (Frankfurt)"
        vcpu="3"
        memory="1"
        storageSize="2"
        os="REDHAT"/>
    <machineImages
        xsi:type="ccl:StorageImage"
        region="FRANKFURT"
        label="Storage machine image 2.2 (Frankfurt)"
        vcpu="9"
        memory="7"
        storageSize="8"
        dbEngine="MARIADB"/>
  </resources>
</ccl:CloudResources>
